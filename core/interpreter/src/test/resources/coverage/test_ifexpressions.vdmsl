operations

suite : () ==> int
	suite () == let
				a = ifexpression(1,2,3),
				b = ifexpression(1,3,2),
				c = ifexpression(3,2,1),
				d = ifexpression(2,1,3),
				e = ifexpression(2,2,3),
				f = ifexpression(2,1,1),
				g = ifexpression(3,2,3)
				in 
				(return 1);

ifexpression : int * int * int ==> int
ifexpression (a,b,c) ==
	if a<>b and a<>c and b<>c
		then return if a>b and a>c
									then a
									elseif b>a and b>c
										then b
										else c
		else return -1